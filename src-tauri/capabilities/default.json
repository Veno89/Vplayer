{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:image:default",
    "core:window:default",
    "core:window:allow-set-size",
    "core:window:allow-outer-size",
    "core:window:allow-inner-size",
    "dialog:allow-open",
    "dialog:allow-confirm",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "updater:default"
  ]
}